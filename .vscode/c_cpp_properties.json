{
    "configurations": [
        {
            "name": "Linux",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/deps/restclient-cpp/include/",
                "${workspaceFolder}/build/_deps/restbed-src/source/",
                "${workspaceFolder}/build/_deps/nlohmann_json-src/include/",
                "${workspaceFolder}/build/_deps/catch2-src/src",
                "${workspaceFolder}/build/_deps/catch2-build/generated-includes/",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [
                "BUILD_SSL"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}